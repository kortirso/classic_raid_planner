= javascript_pack_tag 'guild'
.container#guild
  h2= @guild.full_name(I18n.locale.to_s)
  = react_component('Guild', guild_slug: @guild.slug, guild_id: @guild.id, locale: I18n.locale.to_s, access_token: JwtService.new.json_response(user: Current.user)[:access_token], is_admin: Current.user.is_admin?, is_gm: @user_characters.has_guild_master?, current_user_character_ids: @user_characters.pluck(:id))
